package study;

// スーパークラス
abstract class PolimoSuper {

	protected void call() {
  // 2-2 の要件を満たす処理
	 System.out.println(this.getClass());
	}
// 呼び出し
}
// サブクラス1でインスタンスを生成
PolimoSuper polimo1 = new PolimoSub1();
polimo1.call();
// サブクラス2でインスタンスを生成
PolimoSuper polimo2 = new PolimoSub2();
polimo2.call();